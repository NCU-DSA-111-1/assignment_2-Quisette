	\ifAlphabetOrdinate
		\advance\w by 96\mbox{\tt\char\w}%
	\else
		\advance\w by -1 \mbox{{\ifcase \w 一\or二\or三\or四\or五\or六\or七\or八\or九\fi}}%
	\fi}

